{
  "type": "page",
  "title": "管网监测数据可视化",
  "body": [
    {
      "type": "form",
      "api": "",
      "className": "w-full",
      "body": [
        {
          "type": "grid",
          "columns": [
            {
              "md": 4,
              "body": {
                "type": "select",
                "name": "device_name",
                "label": "选择设备",
                "options": [
                  {"label": "管网监测点2", "value": "管网监测点2"},
                  {"label": "管网监测点5", "value": "管网监测点5"},
                  {"label": "管网监测点3", "value": "管网监测点3"},
                  {"label": "管网监测点1", "value": "管网监测点1"},
                  {"label": "管网监测点4", "value": "管网监测点4"}
                ],
                "value": "管网监测点2"
              }
            },
            {
              "md": 4,
              "body": {
                "type": "select",
                "name": "e_name",
                "label": "选择指标",
                "options": [
                  {"label": "COD", "value": "COD"},
                  {"label": "ORP", "value": "ORP"},
                  {"label": "信号强度", "value": "信号强度"},
                  {"label": "氨氮", "value": "氨氮"},
                  {"label": "水位", "value": "水位"},
                  {"label": "水温", "value": "水温"},
                  {"label": "流速", "value": "流速"},
                  {"label": "流量", "value": "流量"},
                  {"label": "电压", "value": "电压"},
                  {"label": "电导率", "value": "电导率"},
                  {"label": "累计流量", "value": "累计流量"}
                ],
                "value": "COD"
              }
            },
            {
              "md": 4,
              "body": {
                "type": "select",
                "name": "method_name",
                "label": "选择清洗方法",
                "options": [
                  {"label": "中值滤波", "value": "中值滤波"},
                  {"label": "Z-Score滤波", "value": "Z-Score滤波"},
                  {"label": "滑动平均", "value": "滑动平均"}
                ],
                "value": "Z-Score滤波"
              }
            }
          ]
        },
        {
          "type": "iframe",
          "name": "chartFrame",
          "className": "w-full",
          "height": 600,
          "src": "http://127.0.0.1:8000/echarts/chart/device_raw_vs_cleaned/device_name/${device_name}/e_name/${e_name}/method_name/${method_name}/"
        }
      ]
    }
  ]
}